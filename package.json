{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-TrinityCoreAccountPassword": "cd nova-components/TrinityCoreAccountPassword && npm run dev",
        "build-TrinityCoreAccountPassword-prod": "cd nova-components/TrinityCoreAccountPassword && npm run prod",
        "build-game-password": "cd nova-components/GamePassword && npm run dev",
        "build-game-password-prod": "cd nova-components/GamePassword && npm run prod",
        "build-item-display-id": "cd nova-components/ItemDisplayId && npm run dev",
        "build-item-display-id-prod": "cd nova-components/ItemDisplayId && npm run prod",
        "build-item-quality": "cd nova-components/ItemQuality && npm run dev",
        "build-item-quality-prod": "cd nova-components/ItemQuality && npm run prod",
        "build-wow-currency": "cd nova-components/WowCurrency && npm run dev",
        "build-wow-currency-prod": "cd nova-components/WowCurrency && npm run prod",
        "build-trinitycore-compiler": "cd nova-components/TrinitycoreCompiler && npm run dev",
        "build-trinitycore-compiler-prod": "cd nova-components/TrinitycoreCompiler && npm run prod"
    },
    "devDependencies": {
        "axios": "^0.18",
        "cross-env": "^5.1",
        "laravel-mix": "^2.1",
        "laravel-mix-purgecss": "^2.2",
        "laravel-mix-tailwind": "^0.1.0",
        "lodash": "^4.17.5",
        "popper.js": "^1.14.7",
        "resolve-url-loader": "^2.3.2",
        "sass": "^1.17.2",
        "sass-loader": "^7.1.0",
        "tailwindcss": "^0.7.4",
        "vue": "^2.6.7"
    },
    "dependencies": {
        "tailwindcss-gradients": "^1.1.0",
        "vue-template-compiler": "^2.6.7"
    }
}
